{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "http://192.168.1.19:8000"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "simulator": false
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://powercee-monorepo-production.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
